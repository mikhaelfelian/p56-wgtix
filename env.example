# Tripay Payment Gateway Configuration
# Copy this file to .env and fill in your actual Tripay credentials

# Tripay API Configuration
TRIPAY_API_KEY=your_tripay_api_key_here
TRIPAY_PRIVATE_KEY=your_tripay_private_key_here
TRIPAY_MERCHANT_CODE=your_merchant_code_here
TRIPAY_BASE_URL=https://tripay.co.id/api

# For testing, you can use Tripay sandbox
# TRIPAY_BASE_URL=https://tripay.co.id/api-sandbox

# reCAPTCHA Configuration
recaptcha.sitekey=your_recaptcha_site_key_here
recaptcha.secretkey=your_recaptcha_secret_key_here

# Other Application Settings
app.name="Event Registration System"
app.version="1.0.0"
app.environment="development"



